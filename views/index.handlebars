<body>

<!--JUMBOTRON-->
 <div id="jumbotron" class="jumbotron jumbotron-fluid"> 

<!--LOGO-->
    <div id="header">
      <img id="logo-img" src="../public/assets/images/GroupJawr_Logo_B.png" alt="GROUP JAWR Logo">
    </div>


<!--WORKER CARD-->
<div id="home-row" class="row">
  <div class="worker-card" style="width: 60rem;">
      <div class="card-body">
        <h5 class="home-card-title">WORKER</h5> 
          <ul>
            <button type="button" class="btn btn-light">LOG IN</button>
            <button type="button" class="btn btn-light">CREATE PROFILE</button>
          </ul>
      </div>
    </div>
  </div>

<!--CODER CARD-->
  <div id="home-row" class="row">
    <div class="coder-card" style="width: 60rem;">
      <div class="card-body">
        <h5 class="home-card-title">CODER</h5>
          <ul>
            <button type="button" class="btn btn-light">LOG IN</button>
            <button type="button" class="btn btn-light">FIND CODER</button>
            <button type="button" class="btn btn-light">CREATE PROFILE</button>
          </ul>
      </div>
    </div>
  </div>
  
  <!--DONOR CARD-->
  <div id="home-row" class="row">
    <div class="donor-card" style="width: 60rem;">
      <div class="card-body">
        <h5 class="home-card-title">DONOR</h5>
          <ul>
            <button type="button" class="btn btn-light">FIND WORKER</button>
          </ul>
      </div>
    </div>
  </div>
  
  

<!--BODY CONTAINER-->
